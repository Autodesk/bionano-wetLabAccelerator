{
  "name": "Simple Transfer Protocol",
  "parameters": [
    {
      "name": "myPlate",
      "type": "container",
      "value": "ecoli_container",
      "example": "ecoli_container",
      "description": "Container to be used in the protocol"
    },
    {
      "name": "myAbsorbance",
      "type": "length",
      "value": "600:nanometer",
      "example": "600:nanometer",
      "default": "600:nanometer",
      "description" : "Wavelength used for fluorescence readings"
    },
    {
      "name": "growth_plate",
      "type": "container",
      "description": "Plate used for growth curve measurements",
      "value": {
        "isNew": true,
        "id": "",
        "type": "96-deep",
        "storage": "ambient"
      }
    },
    {
      "name": "ecoli_container",
      "type": "container",
      "description": "Reservoir of competent E. coli",
      "value": {
        "isNew": true,
        "id": "",
        "type": "384-flat",
        "storage": "cold_20"
      }
    }
  ],
  "metadata": {},
  "groups": [
    {
      "name": "Transfer Group",
      "inputs": {},
      "metadata": {
        "description": "Transfer contents from one container to another, either 1-to-n or n-to-n"
      },
      "loop": 1,
      "steps": [
        {
          "operation": "transfer",
          "requirements": {},
          "transforms": [
            {
              "wells": "to"
            },
            {
              "wells": "from"
            }
          ],
          "fields": [
            {
              "name": "volume",
              "type": "volume",
              "value": "10.0:microliter"
            },
            {
              "name": "to",
              "type": "aliquot+",
              "singleWell": false,
              "singleContainer": false,
              "value": [
                {
                  "container": "growth_plate",
                  "well": "A2"
                }
              ]
            },
            {
              "name": "from",
              "type": "aliquot+",
              "singleWell": false,
              "singleContainer": false,
              "value": [
                {
                  "container": "ecoli_container",
                  "well": "A1"
                }
              ]
            },
            {
              "name": "dispense_speed",
              "type": "flowrate",
              "optional": true,
              "value": "100:microliter/second"
            },
            {
              "name": "aspirate_speed",
              "type": "flowrate",
              "optional": true,
              "value": "100:microliter/second"
            },
            {
              "name": "mix_before",
              "type": "mixwrap",
              "optional": true,
              "value": {
                "volume": "10.0:microliter",
                "repetitions": 5,
                "speed": "100:microliter/second"
              }
            },
            {
              "name": "mix_after",
              "type": "mixwrap",
              "optional": true,
              "value": {
                "volume": "10.0:microliter",
                "repetitions": 5,
                "speed": "100:microliter/second"
              }
            }
          ]
        }
      ]
    },
    {
      "loop": 3,
      "metadata": {
        "description": "Records incubates and take absorbance measurement"
      },
      "steps": [
        {
          "operation": "incubate",
          "requirements": {},
          "transforms": [
            {
              "container": "object"
            }
          ],
          "fields": [
            {
              "name": "object",
              "type": "container",
              "value": "growth_plate"
            },
            {
              "name": "where",
              "type": "option",
              "options": [
                "ambient",
                "warm_37",
                "cold_4",
                "cold_20",
                "cold_80"
              ],
              "value": "warm_37"
            },
            {
              "name": "duration",
              "type": "duration",
              "value": "60:minute"
            },
            {
              "name": "shaking",
              "type": "boolean",
              "default": false,
              "value": true
            }
          ]
        },
        {
          "operation": "absorbance",
          "requirements": {},
          "transformations": [
            {
              "wells": "wells"
            }
          ],
          "fields": [
            {
              "name": "dataref",
              "type": "string",
              "value": "dataref_${index}"
            },
            {
              "name": "wells",
              "type": "aliquot+",
              "singleContainer": true,
              "value": [
                {
                  "container": "growth_plate",
                  "well": "A1"
                },
                {
                  "container": "growth_plate",
                  "well": "A2"
                }
              ]
            },
            {
              "name": "wavelength",
              "type": "length",
              "value": "600:nanometer",
              "inputAttrs": {
                "min": 300,
                "max": 1000
              }
            },
            {
              "name": "num_flashes",
              "type": "integer",
              "optional": true,
              "default": 25,
              "value": 25
            }
          ]
        }
      ]
    }
  ]
}
