{
  "name": "Mix Example",
  "inputs" : {},
  "metadata": {
    "description" : "Mix specified well using a new pipette tip"
  },
  "loop": 1,
  "steps": [
    {
      "operation": "mix",
      "requirements": {},
      "transforms": [
        {
          "wells": "wells"
        }
      ],
      "fields": [
        {
          "name": "wells",
          "type": "aliquot+",
          "singleContainer" : false,
          "value": [
            {
              "container" : "growth_plate",
              "well" : "A2"
            }
          ]
        },
        {
          "name": "volume",
          "type": "volume",
          "optional" : true,
          "default": "50:microliter",
          "value": "50:microliter"
        },
        {
          "name": "speed",
          "type": "flowrate",
          "optional" : true,
          "default": "100:microliter/second",
          "value": "100:microliter/second"
        },
        {
          "name": "repetitions",
          "type": "integer",
          "optional" : true,
          "default": 10,
          "value": 10
        }
      ]
    }
  ]
}
