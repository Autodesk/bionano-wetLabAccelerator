{
  "name": "Uncover Example",
  "metadata": {
    "description": "Uncover a container"
  },
  "inputs" : {
    "myPlate" : "container"
  },
  "steps": [
    {
      "operation": "uncover",
      "requirements": {},
      "transforms": [
        {
          "container": "object"
        }
      ],
      "fields": [
        {
          "name": "object",
          "type": "container",
          "value": "${myPlate}"
        }
      ]
    }
  ]
}