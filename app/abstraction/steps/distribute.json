{
  "name": "Distribute Example",
  "inputs": {},
  "metadata": {
    "description": "Distribute liquid from source well(s) to destination wells(s)"
  },
  "loop": 1,
  "steps": [
    {
      "operation": "consolidate",
      "requirements": {},
      "transforms": [
        {
          "wells": "to"
        },
        {
          "wells": "from"
        }
      ],
      "fields": [
        {
          "name": "volume",
          "type": "volume",
          "value": "10.0:microliter"
        },
        {
          "name": "to",
          "type": "aliquot+",
          "singleContainer": false,
          "value": [
            {
              "container": "ecoli_container",
              "well": "A1"
            },
            {
              "container": "ecoli_container",
              "well": "B1"
            },
            {
              "container": "ecoli_container",
              "well": "C1"
            }
          ]
        },
        {
          "name": "from",
          "type": "aliquot",
          "value": [
            {
              "container": "growth_plate",
              "well": "A1"
            }
          ]
        },
        {
          "name": "dispense_speed",
          "type": "flowrate",
          "optional": true,
          "default": "100:microliter/second",
          "value": "100:microliter/second"
        },
        {
          "name": "aspirate_speed",
          "type": "flowrate",
          "optional": true,
          "default": "100:microliter/second",
          "value": "100:microliter/second"
        },
        {
          "name": "mix_before",
          "type": "mixwrap",
          "optional": true,
          "value": {
            "volume": "10.0:microliter",
            "repetitions": 10,
            "speed": "100:microliter/second"
          }
        }
      ]
    }
  ]
}
