{
  "name" : "Simple Transfer Protocol",
  "references": [
    {
      "name": "growth_plate",
      "isNew": true,
      "id": "",
      "type": "96-deep",
      "storage": "ambient"
    },
    {
      "name": "ecoli_container",
      "isNew": true,
      "id": "",
      "type": "96-flat",
      "storage": "cold_20"
    }
  ],
  "inputs": {
    "myPlate" : "container",
    "myAbsorbance" : "wavelength"
  },
  "parameters" : {
    "myPlate" : "ecoli_container",
    "myAbsorbance" : "600:nanometer"
  },
  "metadata": {},
  "groups": [
    {
      "name": "Transfer Group",
      "inputs" : {},
      "metadata": {},
      "loop": 1,
      "steps": [
        {
          "operation": "transfer",
          "requirements": {},
          "transforms": [
            {
              "container": "to_container",
              "wells": "to_wells"
            },
            {
              "container": "from_container",
              "wells": "from_wells"
            }
          ],
          "fields": [
            {
              "name": "volume",
              "type": "volume",
              "value": "10.0:microliter"
            },
            {
              "name": "to_container",
              "type": "container",
              "value": "growth_plate"
            },
            {
              "name": "to_wells",
              "type": "well",
              "isArray": true,
              "value": [
                "A2"
              ]
            },
            {
              "name": "from_container",
              "type": "container",
              "value": "ecoli_container"
            },
            {
              "name": "from_wells",
              "type": "well",
              "isArray": true,
              "value": [
                "A1"
              ]
            },
            {
              "name": "mix_after",
              "type": "mixwrap",
              "value": {
                "volume" : "10.0:microliter",
                "repetitions" : 5,
                "speed" : "100:microliter/second"
              }
            }
          ]
        }
      ]
    },
    {
      "loop": 3,
      "metadata" : {
        "description" : "Records incubates and take absorbance measurement"
      },
      "steps": [
        {
          "operation": "incubate",
          "requirements": {},
          "transforms": [
            {
              "container": "object"
            }
          ],
          "fields": [
            {
              "name": "object",
              "type": "container",
              "value": "growth_plate_1"
            },
            {
              "name": "where",
              "type": "temperature",
              "value": "warm_37"
            },
            {
              "name": "duration",
              "type": "string",
              "value": "60:minute"
            },
            {
              "name": "shaking",
              "type": "boolean",
              "value": true
            }
          ]
        },
        {
          "operation": "absorbance",
          "requirements": {},
          "transforms": [
            {
              "container": "object",
              "wells": "wells"
            }
          ],
          "fields": [
            {
              "name": "object",
              "type": "container",
              "value": "growth_plate"
            },
            {
              "name": "wells",
              "type": "well",
              "isArray": true,
              "value": [
                "A1"
              ]
            },
            {
              "name": "wavelength",
              "type": "length",
              "value": "600:nanometer"
            },
            {
              "name": "num_flashes",
              "type": "integer",
              "value": 25
            },
            {
              "name": "dataref",
              "type": "string",
              "value": "dataref_${index}"
            }
          ]
        }
      ]
    }
  ]
}
