  "scripts": {
    "test": "grunt test",
    "docker.prepare": "cd etc ; docker build -t autovirology .",
    "predocker.npm": "rm -rf node_modules",
    "predocker.bower": "rm -rf bower_components",
    "docker.npm": "npm run docker.prepare ; docker run -v `pwd`:/app -w /app autovirology npm install",
    "docker.bower": "npm run docker.prepare ; docker run -v `pwd`:/app -w /app autovirology bower install --allow-root",
    "docker.build": "docker run -v `pwd`:/app -w /app autovirology grunt build",
    "docker.buildall": "npm run docker.npm ; npm run docker.bower ; npm run docker.build",
    "docker.server": "docker run --rm -t -i -p 9000:9000 -v `pwd`:/app -w /app autovirology node server/server.js | bunyan",
    "start": "npm run docker.server",
    "build.standalone.docker": "docker build -t cx1 .",
    "run.standalone.docker": "docker run -t -i -p 9000:9000 cx1",
    "local_server": "RPC_HOST='localhost' RPC_PORT=9000 PORT=8000 node server/server.js"
  }
}
